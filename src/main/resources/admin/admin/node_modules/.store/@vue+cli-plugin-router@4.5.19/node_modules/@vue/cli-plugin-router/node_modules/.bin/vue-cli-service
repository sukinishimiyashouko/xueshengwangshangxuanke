#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/C:/Users/11852/Desktop/1.15高校学生选课管理系统设计与实现/xueshengwangshangxuanke/src/main/resources/admin/admin/node_modules/.store/@vue+cli-service@4.5.19/node_modules/@vue"
else
  export NODE_PATH="$NODE_PATH:$basedir/C:/Users/11852/Desktop/1.15高校学生选课管理系统设计与实现/xueshengwangshangxuanke/src/main/resources/admin/admin/node_modules/.store/@vue+cli-service@4.5.19/node_modules/@vue"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@vue+cli-service@4.5.19/node_modules/@vue/cli-service/bin/vue-cli-service.js" "$@"
else
  exec node  "$basedir/../../../../../../@vue+cli-service@4.5.19/node_modules/@vue/cli-service/bin/vue-cli-service.js" "$@"
fi
